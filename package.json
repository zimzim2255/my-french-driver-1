      "scripts": {
            "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
            "dev:local": "concurrently \"npm run backend:dev\" \"vite --mode local\"",
            "dev:production": "vite --mode production",
            "frontend:dev": "vite --mode local",
            "backend:dev": "cd backend && npm run dev",
            "backend:start": "cd backend && npm start",
            "start": "concurrently \"npm run backend:start\" \"npm run frontend:dev\"",
            "build": "vite build --mode production",
            "build:local": "vite build --mode local",
            "postbuild": "node -e \"const fs=require('fs'); try{fs.copyFileSync('docs/index.html','docs/404.html')}catch(e){}; try{fs.writeFileSync('docs/.nojekyll','')}catch(e){}\"",
            "predeploy": "npm run build",
            "deploy": "git add docs && git commit -m \"Deploy to docs\" || echo no-changes && git push",
            "install:all": "npm install && cd backend && npm install",
            "backend:install": "cd backend && npm install"
      }
